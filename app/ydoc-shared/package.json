{
  "version": "0.1.0",
  "name": "ydoc-shared",
  "private": true,
  "type": "module",
  "author": {
    "name": "Enso Team",
    "email": "contact@enso.org"
  },
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "format": "prettier --version && prettier --write src/ && eslint . --fix",
    "compile": "tsc"
  },
  "exports": {
    "./ast": {
      "source": "./src/ast/index.ts",
      "types": "./dist/ast/index.d.ts",
      "import": "./dist/ast/index.js"
    },
    "./*": {
      "source": "./src/*.ts",
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js"
    }
  },
  "dependencies": {
    "@lezer/common": "^1.2.3",
    "@lezer/markdown": "^1.3.2",
    "@noble/hashes": "^1.6.1",
    "@open-rpc/client-js": "^1.8.1",
    "@types/debug": "^4.1.12",
    "change-case": "^4.1.2",
    "debug": "^4.4.0",
    "enso-common": "workspace:*",
    "fast-diff": "^1.3.0",
    "hash-wasm": "^4.12.0",
    "modern-isomorphic-ws": "^1.0.5",
    "lib0": "^0.2.99",
    "partysocket": "^1.0.3",
    "rust-ffi": "workspace:*",
    "yjs": "^13.6.21",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@fast-check/vitest": "^0.0.8",
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^22.10.4",
    "@types/ws": "^8.5.13",
    "typescript": "^5.7.2",
    "vite-node": "3.0.0-beta.3",
    "vite-plugin-wasm": "^3.4.1",
    "vitest": "3.0.0-beta.3"
  }
}
